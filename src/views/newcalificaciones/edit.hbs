<div class="content-wrap">
  <br>
  <div class="container youplay-content">

    <div class="col-md-9">
      {{#each edit_calificar_primero_segundo}}
      <!-- Billing Information -->
      <h2 class="mt-0">Modificar Calificaciones</h2>
      <form action="/newcalificaciones/edit/{{grado}}/{{id_matricula}}" method="post">

        <input type="hidden" value="{{idnewprimero_segundo}}" name="idnewprimero_segundo" class="form-control" readonly>
        <input type="hidden" value="{{id_matricula}}" name="id_matricula" class="form-control" readonly>
        <input type="hidden" value="{{id_escuela}}" name="id_escuela" class="form-control" readonly>
        <input type="hidden" value="{{grado}}" name="grado" class="form-control" readonly>



        <div class="row">
          <div class="col-md-12">
            <p>Nombre(s):</p>
            <div class="youplay-input">
              <input type="text" value="{{nombres}}" readonly>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <p>Español:</p>
            <div class="youplay-input">
              <input type="number" name="espanol" value="{{espanol}}" placeholder="Español" required>
            </div>
          </div>
          <div class="col-md-12">
            <p>Matematicas:</p>
            <div class="youplay-input">
              <input type="number" name="matematicas" value="{{matematicas}}" placeholder="Matematicas" required>
            </div>
          </div>
          <div class="col-md-12">
            <p>Conocimiento del Medio:</p>
            <div class="youplay-input">
              <input type="number" name="cono_medio" value="{{cono_medio}}" placeholder="Conocimiento del Medio" required>
            </div>
          </div>
          <div class="col-md-12">
            <p>Artes:</p>
            <div class="youplay-input">
              <input type="number" name="artes" value="{{artes}}" placeholder="Artes" required>
            </div>
          </div>
          <div class="col-md-12">
            <p>Educación Socioemocional:</p>
            <div class="youplay-input">
              <input type="number" name="edu_socioemocional" value="{{edu_socioemocional}}" placeholder="Educación Socioemocional" required>
            </div>
          </div>
          <div class="col-md-12">
            <p>Educación Fisica:</p>
            <div class="youplay-input">
              <input type="number" name="edu_fisica" value="{{edu_fisica}}" placeholder="Educación Fisica" required>
            </div>
          </div>
        </div>
        <!-- /Billing Information -->
        <!--Fin de No calificado 1er - 3er Año-->
        <div class="align-right">
          <button class="btn btn-lg">
            Guardar
          </button>
        </div>
      </form>
      {{/each}}

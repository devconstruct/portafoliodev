          <div class="col-md-12">
            <p itemprop="name">Geografia:</p>
            <div class="youplay-input">
              <input type="number" name="geografia" placeholder="Geografia" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Historia:</p>
            <div class="youplay-input">
              <input type="number" name="historia" placeholder="Historia" required>
            </div>
          </div>

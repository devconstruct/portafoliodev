<div class="content-wrap">
  <br>
  <div class="container youplay-content">

    <div class="col-md-9">
      {{#each no_calificadoT1}}
<!--Inicio de No calificado 1er - 2do Año Trimestre 1-->
      <!-- Billing Information -->
      <h2 class="mt-0">Calificar Alumno</h2>
      <form action="/newcalificaciones/calificar/{{grado}}/{{id_matricula}}/{{id_escuela}}/{{grado}}" method="post">

        <div class="row">
          <div class="col-md-12">
            <p>Nombre(s):</p>
            <div class="youplay-input">
              <input type="text" value="{{nombres}}" readonly>
            </div>
          </div>
        </div>

        <div class="row" itemscope itemtype="http://schema.org/EducationalOccupationalCredential">
          <div class="col-md-12">
            <p>Trimestre:</p>
            <div class="youplay-input">
              <input type="text" name="trimestre" value="1" readonly>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Español:</p>
            <div class="youplay-input">
              <input type="number" name="espanol" placeholder="Español" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Matematicas:</p>
            <div class="youplay-input">
              <input type="number" name="matematicas" placeholder="Matematicas" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Conocimiento del Medio:</p>
            <div class="youplay-input">
              <input type="number" name="cono_medio" placeholder="Conocimiento del Medio" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Artes:</p>
            <div class="youplay-input">
              <input type="number" name="artes" placeholder="Artes" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Educación Sociemocional:</p>
            <div class="youplay-input">
              <input type="number" name="edu_socioemocional" placeholder="Educación Socioemocional" required>
            </div>
          </div>
          <div class="col-md-12">
            <p itemprop="name">Educación Fisica:</p>
            <div class="youplay-input">
              <input type="number" name="edu_fisica" placeholder="Educación Fisica" required>
            </div>
          </div>
        </div>
        <!-- /Billing Information -->
        <!--Fin de No calificado 1er - 2do Año Trimestre 1-->
        <div class="align-right">
          <button class="btn btn-lg">
            Guardar
          </button>
        </div>
      </form>
      {{/each}}
    <!-- </div> -->
